#!/usr/bin/env bash

cid=$(docker ps -a|grep bagheera_php|cut -c 1-12)

project_dir=$(cd "$(dirname "$0")" && cd .. && pwd)
project_name=$(basename "$project_dir")

docker-compose up -d

docker exec -ti $cid /var/www/bagheera/bin/build /var/www/bagheera

"$project_dir"/bin/http

docker-compose logs
