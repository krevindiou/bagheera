#!/usr/bin/env bash

project_dir=$(cd "$(dirname "$0")" && cd .. && pwd)
project_name=$(basename "$project_dir")
architecture=$1

if [ 'arm' == "$architecture" ]; then
    docker-compose -f ${project_dir}/docker-compose-arm.yml up -d
else
    docker-compose -f ${project_dir}/docker-compose.yml up -d
fi
