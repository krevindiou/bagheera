{
    "name": "krevindiou/bagheera",
    "license": "proprietary",
    "type": "project",
    "description": "Bagheera, a personal finance manager",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=5.4.0",
        "ext-intl": "*",
        "ext-openssl": "*",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "^4.2",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.2",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.2",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "^4.2",
        "symfony/twig-bundle": "^4.2",
        "symfony/validator": "^4.2"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "symfony/dotenv": "^4.2",
        "symfony/maker-bundle": "^1.11",
        "symfony/profiler-pack": "^1.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "component-dir": "web/components",
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "env-map": {
                "secret": "SECRET",
                "database_host": "POSTGRES_HOST",
                "database_name": "POSTGRES_DB",
                "database_user": "POSTGRES_USER",
                "database_password": "POSTGRES_PASSWORD",
                "database_secure_host": "POSTGRES_SECURE_HOST",
                "database_secure_name": "POSTGRES_SECURE_DB",
                "database_secure_user": "POSTGRES_SECURE_USER",
                "database_secure_password": "POSTGRES_SECURE_PASSWORD"
            }
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "components/flot",
                "type": "component",
                "version": "0.8.1",
                "source": {
                    "url": "https://github.com/flot/flot.git",
                    "type": "git",
                    "reference": "0.8.1"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "jquery.flot.js",
                            "jquery.flot.time.js",
                            "excanvas.min.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/mailcheck",
                "type": "component",
                "version": "1.0.2",
                "source": {
                    "url": "https://github.com/Kicksend/mailcheck.git",
                    "type": "git",
                    "reference": "v1.0.2"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "src/mailcheck.min.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        }
    ]
}
