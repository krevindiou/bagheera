{
    "name": "krevindiou/bagheera",
    "license": "GPL-3.0-only",
    "type": "project",
    "description": "Bagheera, a personal finance manager",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-intl": "*",
        "ext-openssl": "*",
        "components/bootstrap": "~2.3.0",
        "components/flot": "0.8.1",
        "components/font-awesome": "~3.1.0",
        "components/jquery": "~1.8.0",
        "components/jqueryui": "~1.10.0",
        "components/mailcheck": "1.0.2",
        "sensio/framework-extra-bundle": "^5.3",
        "symfony/asset": "4.*",
        "symfony/browser-kit": "4.*",
        "symfony/css-selector": "4.*",
        "symfony/expression-language": "4.*",
        "symfony/flex": "~1.9.4",
        "symfony/form": "4.*",
        "symfony/monolog-bundle": "~3.5.0",
        "symfony/orm-pack": "~2.0.0",
        "symfony/process": "4.*",
        "symfony/security-bundle": "4.*",
        "symfony/swiftmailer-bundle": "~3.2.6",
        "symfony/templating": "4.*",
        "symfony/translation": "4.*",
        "symfony/twig-bundle": "4.*",
        "symfony/validator": "4.*",
        "symfony/webpack-encore-bundle": "~1.7.3",
        "symfony/yaml": "4.*",
        "twig/extensions": "^1.5",
        "white-october/pagerfanta-bundle": "^1.2"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "phpstan/phpstan": "^0.11.2",
        "symfony/dotenv": "4.*",
        "symfony/maker-bundle": "~1.21.1",
        "symfony/phpunit-bridge": "4.*",
        "symfony/profiler-pack": "~1.0.5"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "component-dir": "public/components",
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "components/flot",
                "type": "component",
                "version": "0.8.1",
                "source": {
                    "url": "https://github.com/flot/flot.git",
                    "type": "git",
                    "reference": "0.8.1"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "jquery.flot.js",
                            "jquery.flot.time.js",
                            "excanvas.min.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/mailcheck",
                "type": "component",
                "version": "1.0.2",
                "source": {
                    "url": "https://github.com/Kicksend/mailcheck.git",
                    "type": "git",
                    "reference": "v1.0.2"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "src/mailcheck.min.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        }
    ]
}
