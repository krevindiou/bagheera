{
    "name": "krevindiou/bagheera",
    "license": "proprietary",
    "type": "project",
    "description": "Bagheera, a personal finance manager",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-intl": "*",
        "ext-openssl": "*",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "^4.2",
        "symfony/browser-kit": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.2",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.2",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "^4.2",
        "symfony/twig-bundle": "^4.2",
        "symfony/validator": "^4.2",
        "symfony/webpack-encore-bundle": "^1.1",
        "symfony/yaml": "4.2.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "symfony/dotenv": "^4.2",
        "symfony/maker-bundle": "^1.11",
        "symfony/profiler-pack": "^1.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "component-dir": "web/components",
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "components/flot",
                "type": "component",
                "version": "0.8.1",
                "source": {
                    "url": "https://github.com/flot/flot.git",
                    "type": "git",
                    "reference": "0.8.1"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "jquery.flot.js",
                            "jquery.flot.time.js",
                            "excanvas.min.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/mailcheck",
                "type": "component",
                "version": "1.0.2",
                "source": {
                    "url": "https://github.com/Kicksend/mailcheck.git",
                    "type": "git",
                    "reference": "v1.0.2"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "src/mailcheck.min.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        }
    ]
}
