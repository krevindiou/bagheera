{
    "name": "krevindiou/bagheera",
    "license": "proprietary",
    "type": "project",
    "description": "Bagheera, a personal finance manager",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=5.4.0",
        "ext-intl": "*",
        "ext-openssl": "*",
        "components/bootstrap": "~2.3.0",
        "components/flot": "0.8.1",
        "components/font-awesome": "~3.1.0",
        "components/jquery": "~1.8.0",
        "components/jqueryui": "~1.10.0",
        "components/mailcheck": "1.0.2",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.6",
        "fabpot/goutte": "^3.2",
        "guzzlehttp/guzzle": "^6.3",
        "incenteev/composer-parameter-handler": "^2.0",
        "jms/di-extra-bundle": "1.7.0",
        "jms/security-extra-bundle": "^1.6",
        "nelmio/js-logger-bundle": "^1.4",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/assetic-bundle": "^2.8",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.6.4",
        "symfony/symfony": "3.4.*",
        "twig/extensions": "^1.5",
        "twig/twig": "^1.0||^2.0",
        "white-october/pagerfanta-bundle": "^1.2"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "component-dir": "web/components",
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "env-map": {
                "secret": "SECRET",
                "database_host": "POSTGRES_HOST",
                "database_name": "POSTGRES_DB",
                "database_user": "POSTGRES_USER",
                "database_password": "POSTGRES_PASSWORD",
                "database_secure_host": "POSTGRES_SECURE_HOST",
                "database_secure_name": "POSTGRES_SECURE_DB",
                "database_secure_user": "POSTGRES_SECURE_USER",
                "database_secure_password": "POSTGRES_SECURE_PASSWORD"
            }
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "components/flot",
                "type": "component",
                "version": "0.8.1",
                "source": {
                    "url": "https://github.com/flot/flot.git",
                    "type": "git",
                    "reference": "0.8.1"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "jquery.flot.js",
                            "jquery.flot.time.js",
                            "excanvas.min.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/mailcheck",
                "type": "component",
                "version": "1.0.2",
                "source": {
                    "url": "https://github.com/Kicksend/mailcheck.git",
                    "type": "git",
                    "reference": "v1.0.2"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "src/mailcheck.min.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        }
    ]
}
