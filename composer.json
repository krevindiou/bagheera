{
    "name": "krevindiou/bagheera",
    "license": "proprietary",
    "type": "project",
    "description": "Bagheera, a personal finance manager",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.4.0",
        "ext-mcrypt": "*",
        "ext-openssl": "*",
        "ext-intl": "*",
        "symfony/symfony": "~2.4",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/security-extra-bundle": "~1.5",
        "jms/di-extra-bundle": "~1.4",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "leafo/lessphp": "~0.4",
        "white-october/pagerfanta-bundle": "~1.0",
        "fabpot/goutte": "~2.0",
        "guzzlehttp/guzzle": "~4.0",
        "hautelook/alice-bundle": "~0.1",
        "nelmio/js-logger-bundle": "~1.0",
        "stof/doctrine-extensions-bundle": "~1.1",
        "components/jquery": "~1.8.0",
        "components/jqueryui": "~1.10.0",
        "components/bootstrap": "~2.3.0",
        "components/font-awesome": "~3.1.0",
        "components/flot": "0.8.1",
        "components/mailcheck": "1.0.2"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "component-dir": "web/components"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "components/flot",
                "type": "component",
                "version": "0.8.1",
                "source": {
                    "url": "https://github.com/flot/flot.git",
                    "type": "git",
                    "reference": "0.8.1"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "jquery.flot.js",
                            "jquery.flot.time.js",
                            "excanvas.min.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/mailcheck",
                "type": "component",
                "version": "1.0.2",
                "source": {
                    "url": "https://github.com/Kicksend/mailcheck.git",
                    "type": "git",
                    "reference": "v1.0.2"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "src/mailcheck.min.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        }
    ]
}
