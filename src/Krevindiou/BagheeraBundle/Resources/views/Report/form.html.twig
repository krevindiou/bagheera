{% extends '::base.html.twig' %}

{% block pageTitle 'report_form_page_title'|trans %}

{% block content %}
<a href="{{ path('report_list') }}" class="back">{{ 'back_link'|trans }}</a>

<form action="" novalidate="novalidate" method="post" {{ form_enctype(reportForm) }}>
    {{ form_widget(reportForm) }}

    <div class="form_button">
        <input type="submit" value="{{ 'report_form_submit_button'|trans }}"/>
    </div>
</form>
{% endblock %}
