{% extends '::base.html.twig' %}

{% block pageTitle 'bank_form_page_title'|trans %}

{% block content %}
<a href="{{ path('account_list') }}" class="back">{{ 'back_link'|trans }}</a>

<form action="" novalidate="novalidate" method="post" {{ form_enctype(bankForm) }}>
    {{ form_widget(bankForm) }}

    <div class="form_button">
        <input type="submit" value="{{ 'bank_form_submit_button'|trans }}"/>
    </div>
</form>
{% endblock %}
